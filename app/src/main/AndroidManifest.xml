<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.config.pad.content">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" /> <!-- 项目包名 -->
    <application
        android:name=".libding.config.application.PadApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--SplashActivity-->
        <activity android:name=".ui.MainActivity"  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".libding.rerxmvp.view.activity.PostListRspActivity" />
        <activity android:name=".libding.rerxmvp.view.GetListRspActivity" />
        <activity android:name=".ui.SplashActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.MainSmartActivity" />
        <activity android:name=".ui.crash.CrashInfoActivity"/>
        <activity android:name=".libding.rerxmvp.view.activity.GetMvpRspActivity" />
        <activity android:name=".libding.widget.recyclerlist.TestRecyclerViewBaseActivity" />
        <activity android:name=".ui.LoginTemplateActivity"/>

    </application>

</manifest>